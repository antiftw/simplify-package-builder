{
    "name": "symplify/package-builder",
    "description": "Dependency Injection, Console and Kernel toolkit for Symplify packages.",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "nette/utils": "^3.2",
        "sebastian/diff": "^4.0|^5.0",
        "symfony/config": "^6.2",
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "6.1.*",
        "symfony/finder": "^6.2"
    },
    "require-dev": {
        "symplify/symplify-kernel": "^11.2",
        "phpunit/phpunit": "^9.5.26"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PackageBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PackageBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.2-dev"
        }
    },
    "conflict": {
        "symplify/phpstan-rules": "<11.1.19",
        "symplify/rule-doc-generator-contracts": "<11.1.19",
        "symplify/php-config-printer": "<11.1.19",
        "symplify/autowire-array-parameter": "<11.1.19",
        "symplify/phpstan-extensions": "<11.1.19",
        "symplify/rule-doc-generator": "<11.1.19",
        "symplify/smart-file-system": "<11.1.19",
        "symplify/symfony-static-dumper": "<11.1.19",
        "symplify/config-transformer": "<11.1.19",
        "symplify/coding-standard": "<11.1.19",
        "symplify/easy-parallel": "<11.1.19",
        "symplify/easy-testing": "<11.1.19",
        "symplify/symplify-kernel": "<11.1.19",
        "symplify/easy-ci": "<11.1.19",
        "symplify/monorepo-builder": "<11.1.19",
        "symplify/vendor-patches": "<11.1.19",
        "symplify/easy-coding-standard": "<11.1.19"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
